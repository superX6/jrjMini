.container {
    width: 100%;
    position: relative;
    z-index: 98;
    -webkit-box-sizing: border-box;
       -moz-box-sizing: border-box;
            box-sizing: border-box;
    padding: 0 4%;
    height: 100%;
         background:url('https://www.jrjzx.cn/p2pmini/statics/build/img/star/banner.png') #f0f0f0 no-repeat; 
     
    -webkit-background-size: contain;
            background-size: contain;
}

.panel{
    width: 100%;
    -webkit-box-sizing: border-box;
       -moz-box-sizing: border-box;
            box-sizing: border-box;
    background: #fff;
    padding: 20rpx 20rpx;
    border-radius: 30rpx;
    margin: 20rpx auto;
}
.rule-btn{
    position: fixed;
    width: 115rpx;
    height: 115rpx;
    padding: 18rpx 12rpx;
    -webkit-box-sizing: border-box;
       -moz-box-sizing: border-box;
            box-sizing: border-box;
    border-radius: 50%;
    line-height: 41rpx;
    text-align: center;
    background: #14ac33;
    right: 10rpx;
    top: 30rpx;
    color: #fff;
    font-weight: bold;
    font-size: 32rpx;
    z-index: 99;
}




.cs_button {
    background: transparent;
    width: 100rpx;
    height: 100rpx;
    /* border: 1px solid #14ac33; */
    border: none;
    position: fixed;
    left: 0rpx;
    bottom: 120rpx;
    padding: 0;
    z-index: 9999;
}


.cs_image {
    width: 100rpx;
    height: 100rpx;
    display: block;
}

.rule-btn::before{
    width: 5rpx;
    height: 40rpx;
    background: #14ac33;
    content:'';
    display: block;
    position: absolute;
    z-index: 9999;
    top: -30rpx;
    right: 58rpx;
}
.choose-star{
    margin-top: 235rpx;
    height: 230rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}
.choose-star image{
    display: inline-block;
    width: 180rpx;
    height: 180rpx;
    border-radius: 50%;
    -webkit-flex-basis: 180rpx;
        -ms-flex-preferred-size: 180rpx;
            flex-basis: 180rpx;
    margin-right: 20rpx;
}
.choose-star .text{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
        -ms-flex: 1;
            flex: 1;
}
.choose-star .text .info{
    line-height: 65rpx;
    font-size: 30rpx;
    font-weight: blod;
}

.choose-star .show-btn{
    display: inline-block;
    width: 220rpx;
    height: 100rpx;
    line-height: 95rpx;
    -webkit-flex-basis: 220rpx;
        -ms-flex-preferred-size: 220rpx;
            flex-basis: 220rpx;
    font-size: 36rpx;
    padding: 0;
    text-align: center;
    margin: 40rpx 0 0 0;
    background: #14ac33;
    border: 8rpx solid #b3ff95;
    border-radius:8px;


}
.star-panel{
    height: 520rpx;
}

.list-wrapper{
    width: 100%;
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-wrap: wrap;
        -ms-flex-wrap: wrap;
            flex-wrap: wrap;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
        -ms-flex-pack: justify;
