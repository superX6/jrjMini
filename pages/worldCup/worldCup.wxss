.list-wrapper .list{
    -webkit-flex-basis: 24%;
        -ms-flex-preferred-size: 24%;
            flex-basis: 24%;
    height: 250rpx;
    padding-top: 10rpx;
    -webkit-box-sizing: border-box;
       -moz-box-sizing: border-box;
            box-sizing: border-box;
    /* overflow: hidden; */
}
.list-wrapper .list image{
    display: block;
    width: 140rpx;
    height:140rpx;
    border-radius: 50%;
    margin: 0 auto;

}
.list-wrapper .list image.cur{
    -webkit-box-shadow: 0 0 20rpx #f7d808;
            box-shadow: 0 0 20rpx #f7d808;
}
.list-wrapper .list .star-info.name{
    font-size:27rpx;
    font-weight: blod;
    width: 100%;
    margin-top: 10rpx;

    text-align: center;
    color: #14ac33;
}
.list-wrapper .list .star-info{

    font-weight: blod;
